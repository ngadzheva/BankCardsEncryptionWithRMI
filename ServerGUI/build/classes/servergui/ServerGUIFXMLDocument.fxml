<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" fx:id="apnRootPanel" prefHeight="200" prefWidth="320" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="servergui.ServerGUIController">
   <children>
      <VBox prefHeight="331.0" prefWidth="631.0">
         <children>
            <MenuBar>
              <menus>
                <Menu mnemonicParsing="false" text="File">
                  <items>
                        <MenuItem fx:id="mnuSortedByCardNumber" mnemonicParsing="false" onAction="#onSortedByCardNumber" text="Extract bank cards sorted by card number" />
                        <MenuItem fx:id="mnuSortedByEncryption" mnemonicParsing="false" onAction="#onSortedByEncryption" text="Extract card numbers sorted by encryption" />
                  </items>
                </Menu>
                <Menu mnemonicParsing="false" text="Exit">
                  <items>
                    <MenuItem fx:id="mnuExit" mnemonicParsing="false" onAction="#onExit" text="Exit" />
                  </items>
                </Menu>
              </menus>
               <VBox.margin>
                  <Insets />
               </VBox.margin>
            </MenuBar>
            <TextArea fx:id="txaLog" prefHeight="300.0" prefWidth="640.0">
               <VBox.margin>
                  <Insets top="5.0" />
               </VBox.margin>
            </TextArea>
         </children>
         <padding>
            <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
         </padding>
      </VBox>
   </children>
